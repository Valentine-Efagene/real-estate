# LocalStack Development Environment Variables
# Copy this file to .env and fill in the values
# These values are used by setup-aws.sh to populate SSM parameters in LocalStack

# =============================================================================
# Notification Service - Office365
# =============================================================================
OFFICE365_CLIENT_ID=
OFFICE365_CLIENT_SECRET=
OFFICE365_TENANT_ID=
OFFICE365_SENDER_EMAIL=info@qshelter.ng

# =============================================================================
# Notification Service - SMTP (Mailtrap for testing)
# =============================================================================
SMTP_HOST=smtp.mailtrap.io
SMTP_PORT=2525
SMTP_USERNAME=
SMTP_PASSWORD=
SMTP_ENCRYPTION=STARTTLS

# =============================================================================
# JWT & Encryption
# =============================================================================
ACCESS_TOKEN_SECRET=
REFRESH_TOKEN_SECRET=
ENCRYPTION_PASSWORD=
ENCRYPTION_SALT=

# =============================================================================
# OAuth Providers
# =============================================================================
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# =============================================================================
# Payment Gateway
# =============================================================================
PAYSTACK_SECRET_KEY=
PAYSTACK_PUBLIC_KEY=
