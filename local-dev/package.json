{
    "name": "@real-estate/local-dev",
    "version": "1.0.0",
    "private": true,
    "description": "Local development utilities for QShelter e2e testing with LocalStack",
    "main": "lib/index.ts",
    "scripts": {
        "start": "bash scripts/start.sh",
        "stop": "bash scripts/stop.sh",
        "reset": "bash scripts/reset.sh",
        "logs": "bash scripts/logs.sh",
        "migrate": "bash scripts/migrate.sh",
        "seed": "bash scripts/seed.sh",
        "setup": "bash scripts/setup.sh",
        "setup:full": "bash scripts/setup.sh && pnpm run test:e2e:mortgage",
        "build:shared": "pnpm --filter '../shared/**' run build",
        "build:services": "pnpm --filter '../services/**' run build",
        "build:all": "pnpm run build:shared && pnpm run build:services",
        "deploy:authorizer": "cd ../services/authorizer-service && pnpm run deploy:localstack",
        "deploy:user": "cd ../services/user-service && pnpm run deploy:localstack",
        "deploy:property": "cd ../services/property-service && pnpm run deploy:localstack",
        "deploy:mortgage": "cd ../services/mortgage-service && pnpm run deploy:localstack",
        "deploy:notifications": "cd ../services/notification-service && pnpm run deploy:localstack",
        "deploy:documents": "cd ../services/documents-service && pnpm run deploy:localstack",
        "deploy:all": "pnpm run deploy:authorizer && pnpm run deploy:user && pnpm run deploy:property && pnpm run deploy:mortgage && pnpm run deploy:notifications && pnpm run deploy:documents",
        "test:e2e": "pnpm run start && pnpm run migrate && pnpm --filter '../services/**' run test:e2e",
        "test:e2e:mortgage": "pnpm --filter ../services/mortgage-service run test:e2e",
        "test:e2e:user": "pnpm --filter ../services/user-service run test:e2e",
        "test:e2e:deployed": "pnpm --filter ../services/mortgage-service run test:e2e:deployed",
        "test:e2e:deployed:verbose": "API_BASE_URL=http://vf7kbi2plw.execute-api.localhost.localstack.cloud:4566/test pnpm --filter ../services/mortgage-service run test:e2e -- --verbose"
    },
    "dependencies": {
        "@aws-sdk/client-cloudwatch-logs": "^3.700.0",
        "@aws-sdk/client-dynamodb": "^3.700.0",
        "@aws-sdk/client-eventbridge": "^3.700.0",
        "@aws-sdk/client-lambda": "^3.700.0",
        "@aws-sdk/client-s3": "^3.700.0",
        "@aws-sdk/client-secrets-manager": "^3.700.0",
        "@aws-sdk/client-sns": "^3.700.0",
        "@aws-sdk/client-sqs": "^3.700.0",
        "@aws-sdk/client-ssm": "^3.700.0",
        "@valentine-efagene/qshelter-common": "^2.0.26",
        "jsonwebtoken": "^9.0.2"
    },
    "devDependencies": {
        "@types/jsonwebtoken": "^9.0.7",
        "@types/node": "^22.10.2",
        "typescript": "^5.7.2"
    }
}