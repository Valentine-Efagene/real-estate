# Node Environment
NODE_ENV=development

# Server
PORT=3001

# Database
DB_HOST=localhost
DB_PORT=3306
DB_NAME=qshelter_users
DB_USERNAME=root
DB_PASSWORD=password

# Authentication
JWT_SECRET=your-jwt-secret-here-change-in-production
REFRESH_TOKEN_SECRET=your-refresh-token-secret-here-change-in-production

# Encryption
ENCRYPTION_PASSWORD=your-encryption-password-32-chars-min
ENCRYPTION_SALT=your-encryption-salt-16-chars-min

# OAuth
GOOGLE_CLIENT_ID=your-google-client-id

# URLs
FRONTEND_BASE_URL=http://localhost:3000
BASE_URL=http://localhost:3001

# AWS
AWS_REGION=us-east-1

# Event Bus
EVENT_BUS_NAME=qshelter-event-bus

# Lambda Authorizer
AUTHORIZER_LAMBDA_ARN=arn:aws:lambda:us-east-1:123456789:function:qshelter-authorizer-dev
