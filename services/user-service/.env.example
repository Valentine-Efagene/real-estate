# Application
NODE_ENV=production
PORT=3000

# Infrastructure values from CDK stack outputs
VPC_ID=vpc-xxxxxxxxx
DB_SECURITY_GROUP_ID=sg-xxxxxxxxx
PRIVATE_SUBNET_ID_1=subnet-xxxxxxxxx
PRIVATE_SUBNET_ID_2=subnet-xxxxxxxxx
HTTP_API_ID=xxxxxxxxxx

# Database Configuration (from CDK outputs)
DB_HOST=your-rds-endpoint.rds.amazonaws.com
DB_PORT=3306
DATABASE_SECRET_ARN=arn:aws:secretsmanager:us-east-1:ACCOUNT_ID:secret:xxxxx

# Redis/Valkey (from CDK outputs)
REDIS_HOST=your-valkey-endpoint.amazonaws.com
REDIS_PORT=6379

# DynamoDB (from CDK outputs)
ROLE_POLICIES_TABLE_NAME=role-policies

# EventBridge (from CDK outputs)
EVENTBRIDGE_BUS_NAME=qshelter-event-bus

# S3 (from CDK outputs)
S3_BUCKET_NAME=qshelter-uploads-ACCOUNT_ID

# JWT Configuration
JWT_SECRET=your-jwt-secret-here-change-in-production
REFRESH_TOKEN_SECRET=your-refresh-token-secret-here-change-in-production

# Encryption
ENCRYPTION_PASSWORD=your32charlongencryptionkeyhere!
ENCRYPTION_SALT=your16charlongivhere

# OAuth - Google
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=https://api.qshelter.ng/auth/google/callback

# Frontend
FRONTEND_BASE_URL=https://www.qshelter.ng
BASE_URL=https://api.qshelter.ng

# Branding
INFO_EMAIL=info@qshelter.ng
SUPPORT_EMAIL=support@qshelter.ng
APP_LINK=https://www.qshelter.ng
