# Environment variables for LocalStack
# Copy to .env.localstack and fill in values

NODE_ENV=localstack
STAGE=localstack

# Database
DB_HOST=127.0.0.1
DB_PORT=3307
DB_USER=root
DB_PASSWORD=rootpassword
DB_NAME=qshelter_test

# AWS/LocalStack
